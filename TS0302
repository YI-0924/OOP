#include<iostream>
#include<fstream>
#include<string>
#include<vector>
#include<sstream>

using namespace std;

int main() {
	ifstream file;
	file.open("grade.txt");
	ofstream inFile;
	inFile.open("grades.Output");
	char input[200];
	while (file.getline(input,200,'\n'))
	{	
		vector<int> data;
		for (int i = 0; i < 200; i++) {
			if (input[i] - '0' <= 5 && input[i] - '0' >= 0 && input[i] != ' ') {
				data.push_back(input[i] - '0');
			}
			else {
				continue;
			}
		}
		int count[6] = { 0 };
		int size = data.size();
		for (int j = 0; j < size; j++) 
		{
			switch (data[j]) 
			{
			case 0:
				count[0]++;
				break;
			case 1:
				count[1]++;
				break;
			case 2:
				count[2]++;
				break;
			case 3:
				count[3]++;
				break;
			case 4:
				count[4]++;
				break;
			case 5:
				count[5]++;
				break;
			}
			data[j] = 0;
		}
		for (int i = 0; i <= 5; i++) 
		{
			inFile << count[i] << " grade(s) of " << i << "\n";
		}
	}
	file.close();
	inFile.close();
	return 0;
}
